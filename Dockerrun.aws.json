{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "front",
      "image": "hmada15/multi-front",
      "hostname": "front",
      "essential": false,
      "memory": 128
    },
    {
      "name": "api",
      "image": "hmada15/multi-api",
      "hostname": "api",
      "essential": true,
      "memory": 128
    },
    {
      "name": "worker",
      "image": "hmada15/multi-worker",
      "hostname": "worker",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "hmada15/multi-nginx",
      "hostname": "nginx",
      "essential": false,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "front",
        "server"
      ],
      "memory": 128
    }
  ]
}